  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "vue"
    ],
    "preset": "ts-jest/presets/js-with-babel",
    "transform": {
      ".*\\.js$": "babel-jest",
      ".*\\.vue$": "vue-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!vue-awesome|vue-multiselect|vue-resize-split-pane).+\\.js$"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "globals": {
      "ts-jest": {
        "compiler": "ttypescript",
        "tsConfig": "./tsconfig.json"
      }
    },
    "setupFiles": [
      "<rootDir>/test/unit/setup.js"
    ],
    "collectCoverageFrom": [
      "**/src/**/*.{vue,js,ts}",
      "!**/node_modules/**",
      "!**/docs/**",
      "!**/unknown"
    ]
  },
